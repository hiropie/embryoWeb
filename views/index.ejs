<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <p id="num">1</p>
    <img  src="/images/basis/first.png" id="embryo_gif" width="720" height="480"> 
    <button id="gifStart">再生</button>
    <table id="parameter">
      <tr align="center">
        <th>気温</th>
        <th>湿度</th>
      </tr>
      <tr align="center">
        <td id="tmp">0</td>
        <td id="hum">0</td>
      </tr>
    </table>
    <form id="pushDispenser">
      <button id="push" type="submit">1プッシュ</button>
    </form>
    <form id="gifChange">
      <button id="reload" type="submit">reload</button>
    </form>


    <div id="loading">
      <div class="spinner"></div>
    </div>

    <script src="/socket.io/socket.io.js"></script>
    <script src="https://code.jquery.com/jquery-1.11.1.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.3.0/socket.io.js"></script>
    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js"
        integrity="sha256-pasqAKBDmFT4eHoN2ndd6lN370kFiGUFyTiUHWhU7k8="
        crossorigin="anonymous"></script>
    <script src="javascripts/server-socket.js"></script>
    <script>
      var accessCount = 1;

      window.onload = ()=>{
        setTimeout(()=>{
          const spinner = document.getElementById('loading');
          spinner.classList.add('loaded');
        },60000);
        //loadGif(accessCount);
      };
      
      $('#gifStart').on('click', function(){
        nowSrc = $('img').attr('src');
        $('img').attr('src', nowSrc);
      });
    </script>
  </body>
</html>