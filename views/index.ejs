<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <p id="num">1</p>
    <img  src="/images/actionScreen.gif" id="embryo_gif" width="720" height="480"> 
    <ul id="messages"></ul>
    <form>
      <button id="reload" type="submit">reload</button>
    </form>


    <div id="loading">
      <div class="spinner"></div>
    </div>

    <script src="/socket.io/socket.io.js"></script>
    <script src="https://code.jquery.com/jquery-1.11.1.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.3.0/socket.io.js"></script>
    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js"
        integrity="sha256-pasqAKBDmFT4eHoN2ndd6lN370kFiGUFyTiUHWhU7k8="
        crossorigin="anonymous"></script>
    <script src="javascripts/index.js"></script>
    <script>
      var accessCount = 1;

      window.onload = ()=>{
        setTimeout(()=>{
          const spinner = document.getElementById('loading');
          spinner.classList.add('loaded');
        },25000);
        //loadGif(accessCount);
      };
      
      document.getElementById(load).onclick = loadGif();

      function loadGif(num){
        document.getElementById(embryo_gif).src = '/images/gifs/actionScreen'+(num)+'.gif'
      }
    </script>
  </body>
</html>