<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body class="container">
    
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <div class="container" id="loading">
      <div class="spinner"></div>
    </div>
    <img class="container" src='http://localhost:3000/images/actionScreen.gif' id="embryo_gif" width="720" height="480"> 
    <input id="load" class="btn btn-primary" type="submit" value="更新">

    <script src="/socket.io/socket.io.js"></script>
    <script src="https://code.jquery.com/jquery-1.11.1.js"></script>
    <script>
      window.onload = ()=>{
        setTimeout(()=>{
          const spinner = document.getElementById('loading');
          spinner.classList.add('loaded');
        },30000);
      };
      console.log(count);
      document.getElementById(load).onclick = loadGif();

      function loadGif(){
        document.getElementById(embryo_gif).src = 'http://localhost:3000/images/actionScreen.gif'
      }
    </script>
  </body>
</html>